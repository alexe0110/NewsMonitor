          },
          {
            "matcher": { "id": "byName", "options": "title" },
            "properties": [
              { "id": "custom.width", "value": 400 }
            ]
          }
        ]
      },
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true
      },
      "targets": [
        {
          "datasource": { "type": "grafana-clickhouse-datasource", "uid": "clickhouse-news" },
          "format": 1,
          "queryType": "sql",
          "rawSql": "SELECT title, category, round(confidence, 2) as confidence, source, formatDateTime(published_at, '%Y-%m-%d %H:%M') as published FROM news_classified ORDER BY published_at DESC LIMIT 20",
          "refId": "A"
        }
      ]
    }
